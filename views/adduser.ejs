    <div class="container">
        <form action="/admin/adduser" method="post">
           
            <div class="form-group" style="padding: 10px">
              <label for="username">Voter Email</label>
              <input required type="email" class="form-control" id="username" name="username" aria-describedby="emailHelp" placeholder="Enter voter Email">
            </div>
            <div class="form-group" style="padding: 10px">
              <label for="userage">Voter age</label>
              <input required type="number" class="form-control"  id="userage" name="userage" aria-describedby="emailHelp" placeholder="Enter voter age" onkeyup="confirmpass()" onkeydown="confirmpass()">
            </div>
            <div class="form-group" style="padding: 10px">
              <label for="useraddress">Voter Address</label>
              <input required type="text" class="form-control" id="useraddress" name="useraddress" aria-describedby="emailHelp" placeholder="Enter Voter address">
            </div>
            <div class="form-group" style="padding: 10px">
              <label for="exampleInputPassword1">Password</label>
              <input required type="password" class="form-control" id="adminpassword" placeholder="Password" name="adminpassword">
            </div>
            <div class="form-group" style="padding: 10px">
              <label for="exampleInputPassword1">Confirm Password</label>
              <input required type="password" class="form-control" id="confirmpassword" placeholder="Password" name="confirmpassword" onkeydown="confirmpass()" onkeyup="confirmpass()">
            </div>
            <div style="padding: 10px;"> 
                  <div style="padding-top: 15px;">
                    <button disabled id="btn" type="submit" class="btn btn-primary" name="addadmin" value="">Submit</button>
                    <div style="color: red;font-weight: bold;"><%=msg%></div>
                  </div>
            </div>
            
          </form>

    </div>
    <div class="container">
      <center><button class="btn btn-info" type="button" id="showactiveusers" name="showactiveusers">Show Users</button></center>
      <div class="table-responsive" style="padding: 10px;">
        <table class="table table-bordered">
          <thead>
            <th>User id</th>
            <th>admin Name</th>
            <th>User Address</th>
          </thead>
          <tbody id="result">
          
          </tbody>
        </table>
      </div>
    </div>

