    <script>
      function confirmpass()
      {
          if(document.getElementById('confirmpassword').value==document.getElementById('adminpassword').value && document.getElementById('userage').value>=18)
          {
            document.getElementById('btn').disabled=false
          }
          else
          {
            document.getElementById('btn').disabled=true
          }
      }

      function checkage()
      {
          if(document.getElementById('userage').value>=18)
          document.getElementById('btn').disabled=false
      }
    </script>
